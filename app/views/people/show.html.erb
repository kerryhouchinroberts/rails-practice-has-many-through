<div class="page-header">
  <h1>Showing Person <%= @person.name %></h1>
</div>

<h2>Organizations</h2>

<table>
  <thead>
    <tr>
      <th>
        Name
      </th>
    </tr>
  </thead>
  <tbody>
    <div class="well">
      <tr>
        <% @person.organizations.each do |organization| %>
        <td><%= organization.name %> </td>
      </tr>
        <% end %>
    </div>
  </tbody>
</table>
